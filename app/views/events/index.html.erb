<div class='row'>
  <div class='col-sm-10 offset-sm-1'>
    <div class='card'>
      <div class='card-header'>
        Events
      </div>
      <div class='card-body'>
        <table class='table table-striped table-sm table-bordered'>
          <thead>
            <tr>
              <th>Command</th>
              <th>Data</th>
              <th>Description</th>
              <th>Timestamp</th>
              <th>Priority</th>
              <th>Partition</th>
              <th>Zone</th>
            </tr>
          </thead>
          <tbody>
            <% @events.each do |event| %>
              <tr>
                <td><%= event.command %></td>
                <td><%= event.data %></td>
                <td><%= event.description %></td>
                <td><%= event.timestamp %></td>
                <td><%= event.priority %></td>
                <td><%= event.partition %></td>
                <td><%= event.zone %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
